{
    "manifestVersion": 1.0,
    "id": "RESTRequestButton",
    "publisher": "Apodemus",
    "version": "1.0.115",
    "public": true,
    "name": "Work Item REST Request Button",
    "description": "Adds a custom control with a button that sends a REST request.",
    "categories": [
        "Azure Boards"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "restbutton-128.png",
        "large": "restbutton-512.png"
    },
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "scopes": [
        "vso.work"
    ],
    "files": [
        {
            "path": "static",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        }
    ],
    "links": {
        "home": {
            "uri": "https://github.com/Zt-freak/DevOpsWorkItemRESTRequestButton"
        },
        "learn": {
            "uri": "https://github.com/Zt-freak/DevOpsWorkItemRESTRequestButton"
        },
        "repository": {
            "uri": "https://github.com/Zt-freak/DevOpsWorkItemRESTRequestButton"
        },
        "issues": {
            "uri": "https://github.com/Zt-freak/DevOpsWorkItemRESTRequestButton/issues"
        },
        "support": {
            "uri": "https://github.com/Zt-freak/DevOpsWorkItemRESTRequestButton/issues"
        }
    },
    "tags": [
        "Work Items",
        "Extensions",
        "Work Item Control",
        "REST"
    ]
}
